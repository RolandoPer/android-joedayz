<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="wrap_content"
	android:orientation="vertical" style="@style/MiTema" android:padding="20dip">
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="Usuario" style="@style/Texto" />
	<EditText android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:id="@+id/txtUsuario"
		android:autoText="false" />
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="Clave" style="@style/Texto" />
	<EditText android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:id="@+id/txtClave"
		android:autoText="false" android:password="true" />
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<Button android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/btnIniciarSesion"
		android:text="Iniciar Sesion" android:drawableLeft="@drawable/user"
		android:padding="15dip" style="@style/Boton" android:onClick="btnIniciarSesion_Click" />
	<Button android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/btnNuevoUsuario"
		android:text="Inscribete Aqui" android:drawableLeft="@drawable/edit"
		android:padding="15dip" style="@style/Boton" android:onClick="btnNuevoUsuario_Click" />
	<Button android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:id="@+id/btnRegresar"
		android:text="Regresar" android:drawableLeft="@drawable/undo"
		android:padding="15dip" style="@style/Boton" android:onClick="btnRegresar_Click" />
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
</LinearLayout>